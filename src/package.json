{
  "name": "freebusy-api",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "cp .env .dev.vars 2>/dev/null || true; wrangler dev --config wrangler.toml",
    "deploy": "wrangler deploy --config wrangler.toml",
    "build": "tsc --noEmit",
    "check": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20241205.0",
    "@vitest/coverage-v8": "^1.6.1",
    "typescript": "^5.6.3",
    "vitest": "^1.6.0",
    "wrangler": "^3.83.0"
  }
}
